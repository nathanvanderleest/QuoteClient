<Window x:Class="QuoteClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:QuoteClient"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="quoteButton" Content="Button"  Margin="10,10,10,250" Click="OnGetQuote" />
        <TextBox x:Name="textQuote" HorizontalAlignment="Center" Height="23" Margin="10,10" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Center" Width="120"/>


    </Grid>
</Window>
